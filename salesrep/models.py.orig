from django.db import models
from core.models import Person, Contact, Address, TimeStamped


class SalesRep(Person):
    is_inactive = models.BooleanField()
    #password

class CommissionTag(models.Model):
	tag = models.CharField(max_length=3)
	
class CommissionTagSplit(models.Model):
	sales_rep = models.ForeignKey(SalesRep)
	commission_tag = models.ForeignKey(CommissionTag)
	commission_percent = models.FloatField()
	is_inactive = models.BooleanField()
	
	
